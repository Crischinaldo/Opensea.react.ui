{
  "name": "opensea-react-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^5.0.0",
    "classnames": "^2.3.1",
    "framer-motion": "^4.1.17",
    "node-sass": "^6.0.0",
    "node-sass-package-importer": "^5.3.2",
    "npm": "^7.16.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-faq-component": "^1.3.1",
    "react-icons-kit": "^1.3.1",
    "react-intersection-observer": "^8.32.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-spinners-kit": "^1.9.1",
    "reactstrap": "^8.9.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/styles.scss src/assets/css/styles.css",
    "minify-sass": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/styles.scss src/assets/css/styles.min.css --output-style compressed",
    "map-sass": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/styles.scss src/assets/css/styles.css --source-map true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
